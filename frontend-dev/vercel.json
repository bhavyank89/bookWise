{
    "version": 2,
    "public": true,
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "env": {
        "REACT_APP_SERVER_URL": "http://localhost:4000",
        "REACT_APP_ADMIN_URL": "http://localhost:3001",
        "REACT_APP_MAIN_URL": "http://localhost:3000"
    }
}